      <td width = '20%'> -- </td>
      <td width = '20%'><%= number_with_delimiter(@avgwtdfootage) %></td>
      <td width = '20%'><%= number_with_delimiter(@avgmtdfootage) %></td>
    </tr>
    <tr>
