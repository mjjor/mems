      <td width = '20%'><%= number_with_delimiter(@avgwtdsqfootage) %></td>
      <td width = '20%'><%= number_with_delimiter(@avgmtdsqfootage) %></td>
    </tr>
    <tr>
      <th width = '20%' style = "background-color: #F5F5F5">Poundage</th> 
