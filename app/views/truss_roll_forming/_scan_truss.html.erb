<% @page_module = 'production' %>
<% @page_sub_module = 'truss_rollformer' %>
<% case @trans_code 
      when "VRS" then @page_access = 'versa' 
      when "OPA" then @page_access = 'offline_panel'
      when "NCN" then @page_access = 'scan_truss' 
   end %>

<%= error_messages_for(@truss_roll_forming) %>

    
    <div class="small-4 columns">
      <%= f.label(:trusslabel, "Truss Label", class: "prefix") %>
     </div>
    <div class="small-8 columns">    
        <%= f.text_field(:trusslabel) %>
        <%= f.hidden_field :trans_code, :value => @trans_code %>
        <%= f.hidden_field :users_id, :value => session[:user_id] %>
        <%= f.hidden_field :workstation, :value => 3 %>
    </div>
    
<center>
<%= @prodprocessed %>
</center>

