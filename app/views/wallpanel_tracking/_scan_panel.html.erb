<% @page_module = 'production' %>
<% @page_sub_module = 'wallpanel' %>
<% case @trans_code 
      when "VRS" then @page_access = 'versa' 
      when "OPA" then @page_access = 'offline_panel'
      when "NCN" then @page_access = 'nucon'
      when "STH" then @page_access = 'sheathing' 
   end %>

<%= error_messages_for(@versatable) %>

<div class="row">
  <div class="small-6 small-centered medium-6 medium-centered large-6 large-centered columns ">
    <div class="small-5 columns">
      <%= f.label(:panellabel, "Panel Label", class: "prefix") %>
    </div>
    <div class="small-7 columns">
        <%= f.text_field(:panellabel) %>
        <%= f.hidden_field :trans_code, :value => @trans_code %>
        <%= f.hidden_field :users_id, :value => session[:user_id] %>
    </div>
  </div>
</div>